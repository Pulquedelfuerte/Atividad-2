dia if 
let L = 5 ;

if (L ===1){
    console.log("Lunes");
}else if (L===2){
    console.log("Martes");
}else if(L===3){
    console.log("Miercoles");
}else if (L===4){
    console.log("Jueves")
}else if (L===5){
    console.log("Viernes");
}else if(L===6){
    console.log("Sabado");
}else if (L===7){
    console.log("Domingo")
}
